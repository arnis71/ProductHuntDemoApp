<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="350" android:interpolator="@android:anim/accelerate_decelerate_interpolator">

    <rotate android:fromDegrees="10" android:toDegrees="0"/>

    <translate
        android:fromYDelta="60%p"
        android:toYDelta="0%p" />

    <alpha
        android:fromAlpha="0.0"
        android:toAlpha="1.0" />
</set>